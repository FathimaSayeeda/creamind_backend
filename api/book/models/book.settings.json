{
  "connection": "default",
  "collectionName": "books",
  "info": {
    "name": "Book"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "coverImage": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "images": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "author": {
      "model": "author"
    },
    "age_groups": {
      "collection": "age-group"
    },
    "categories": {
      "collection": "category"
    },
    "publisher": {
      "model": "publisher"
    },
    "RetailPrice": {
      "type": "integer"
    },
    "book_series": {
      "model": "book-series"
    },
    "BookSeriesIdx": {
      "type": "integer"
    }
  }
}
